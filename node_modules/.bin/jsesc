#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/jsesc/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/jsesc@2.5.2/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules:/home/workspace/react-hooks/sessions/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/jsesc/bin/jsesc/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/jsesc/bin/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/jsesc/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jsesc/bin/jsesc" "$@"
else 
  exec node  "$basedir/../jsesc/bin/jsesc" "$@"
fi
