#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/autoprefixer@10.4.7_postcss@8.4.13/node_modules/autoprefixer/bin/autoprefixer/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/autoprefixer@10.4.7_postcss@8.4.13/node_modules/autoprefixer/bin/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/autoprefixer@10.4.7_postcss@8.4.13/node_modules/autoprefixer/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/autoprefixer@10.4.7_postcss@8.4.13/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules:/home/workspace/react-hooks/sessions/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/autoprefixer/bin/autoprefixer/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/autoprefixer/bin/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/autoprefixer/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
else 
  exec node  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
fi
