#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/ts-node@10.7.0/node_modules/ts-node/dist/bin-script-deprecated.js/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/ts-node@10.7.0/node_modules/ts-node/dist/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/ts-node@10.7.0/node_modules/ts-node/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/ts-node@10.7.0/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules:/home/workspace/react-hooks/sessions/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/cosmiconfig-typescript-loader@1.0.9/node_modules/cosmiconfig-typescript-loader/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/cosmiconfig-typescript-loader@1.0.9/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ts-node@10.7.0/node_modules/ts-node/dist/bin-script-deprecated.js" "$@"
else 
  exec node  "$basedir/../../../../../ts-node@10.7.0/node_modules/ts-node/dist/bin-script-deprecated.js" "$@"
fi
