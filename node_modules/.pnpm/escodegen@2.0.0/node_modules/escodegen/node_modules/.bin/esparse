#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/esparse.js/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/esprima@4.0.1/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules:/home/workspace/react-hooks/sessions/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/escodegen@2.0.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esparse.js" "$@"
else 
  exec node  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esparse.js" "$@"
fi
