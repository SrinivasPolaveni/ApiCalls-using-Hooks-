#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/acorn@8.7.1/node_modules/acorn/bin/acorn/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/acorn@8.7.1/node_modules/acorn/bin/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/acorn@8.7.1/node_modules/acorn/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/acorn@8.7.1/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules:/home/workspace/react-hooks/sessions/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/terser@5.13.1/node_modules/terser/node_modules:/home/workspace/react-hooks/sessions/making-api-call-with-hooks-part-2-final/node_modules/.pnpm/terser@5.13.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@8.7.1/node_modules/acorn/bin/acorn" "$@"
else 
  exec node  "$basedir/../../../../../acorn@8.7.1/node_modules/acorn/bin/acorn" "$@"
fi
